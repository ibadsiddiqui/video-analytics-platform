{
  "version": 2,
  "buildCommand": "yarn vercel-build",
  "outputDirectory": "dist",
  "installCommand": "yarn install",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}
